<!DOCTYPE settings SYSTEM "../settings.dtd">
<settings>
  <dl_settings>
    <dl_settings NAME="Fisheye">
    <dl_setting var="imgRot" min="0.0" 	step="1" 	max="360" 	module="computer_vision/cv_bebop_stabilization" shortname="image rotation"/>
      <dl_setting var="frameRot" 	min="0.0" 	step="1" 	max="360" 	module="computer_vision/cv_bebop_stabilization" shortname="frame rotation"/>
      <dl_setting var="focalLength" min="0.01" 	step="0.001" 	max="5" 	module="computer_vision/cv_bebop_stabilization" shortname="focal length"/>
      <dl_setting var="k_fisheye" 	min="1" 	step="0.001" 	max="5" 	module="computer_vision/cv_bebop_stabilization" shortname="k"/>
      <dl_setting var="scale_x"  	min="0.01" 	step="0.01" 	max="2" 	module="computer_vision/cv_bebop_stabilization" shortname="Scale x"/>
      <dl_setting var="scale_y"  	min="0.01" 	step="0.01" 	max="2" 	module="computer_vision/cv_bebop_stabilization" shortname="Scale y"/> 
      <dl_setting var="near" 	 		min="0.0001" 	step="0.001" 	max="1" 	module="computer_vision/cv_bebop_stabilization" shortname="near"/>
      <dl_setting var="far"     	 	min="1" 		step="0.1" 		max="100" 	module="computer_vision/cv_bebop_stabilization" shortname="far"/>
      <dl_setting var="angleOfView"  	min="1" 		step="1" 		max="180" 	module="computer_vision/cv_bebop_stabilization" shortname="Angle of view"/> 
      <dl_setting var="noroll" 	 		min="0" 	step="1" 	max="1" 	module="computer_vision/cv_bebop_stabilization" shortname="No roll" values="FALSE|TRUE"/>
      <dl_setting var="nopitch" 	 	min="0" 	step="1" 	max="1" 	module="computer_vision/cv_bebop_stabilization" shortname="No pitch" values="FALSE|TRUE"/>
      <dl_setting var="noprojection" 	min="0" 	step="1" 	max="1" 	module="computer_vision/cv_bebop_stabilization" shortname="No projection" values="FALSE|TRUE"/>
    </dl_settings>
  </dl_settings>
</settings>
  