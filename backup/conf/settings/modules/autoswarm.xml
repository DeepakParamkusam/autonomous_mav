<!DOCTYPE settings SYSTEM "../settings.dtd">
<settings>
  <dl_settings>
    <dl_settings NAME="Swarm">
    	<dl_setting var="AUTOSWARM_VMAX" 		min="0.1" 	step="0.1" 	max="20" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Max Vel. (m/s)"/> 
      	<dl_setting var="AUTOSWARM_YAWRATEMAX" 	min="0" 	step="1" 	max="360" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Max Yaw (deg/s)"/>
    	<dl_setting var="AUTOSWARM_MODE" 		min="1" 	step="1" 	max="2" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Swarm Mode" values="See where you fly|Look towards global attractor"/>
      	<dl_setting var="AUTOSWARM_SEPERATION" 	min="1" 	step="0.1" 	max="2.5" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Seperation (m)"/>
      	<dl_setting var="AUTOSWARM_ATTRACTOR" 	min="0" 	step="1" 	max="3" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Attractor mode" values="POINT|BUCKET|CW CIRCLE|CC CIRCLE"/>
      	<dl_setting var="AUTOSWARM_CIRCLE_R" 	min="0.5" 	step="0.1" 	max="2.5" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Circle radius (m)"/> 
      	<dl_setting var="AUTOSWARM_GLOBAL" 		min="0" 	step="0.01" max="1" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Gl. Str. (% vmax)"/>
		<dl_setting var="AUTOSWARM_E" 			min="0.0001" step="0.0001" max="0.02" module="computer_vision/autoswarm/autoswarm_opencv" shortname="Repulsive Str."/>
      	<dl_setting var="AUTOSWARM_EPS" 			min="0.01" 	step="0.01" max="0.1" 	module="computer_vision/autoswarm/autoswarm_opencv" shortname="Diff. Str."/>      		
    </dl_settings>
  </dl_settings>
</settings>
